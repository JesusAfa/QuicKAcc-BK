{
  "version": 2,
  "env": {
    "NODE_ENV": "@NODE_ENV_VERCEL",
    "DB_HOST": "@DB_HOST_VERCEL",
    "DB_PORT": "@DB_PORT_VERCEL",
    "DB_USERNAME": "@DB_USERNAME_VERCEL",
    "DB_PASSWORD": "@DB_PASSWORD_VERCEL",
    "JWT_SECRET": "@JWT_SECRET_VERCEL",
    "DB_DATABASE": "@DB_DATABASE_VERCEL"
  },
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ]
}
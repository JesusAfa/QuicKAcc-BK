{
  "version": 2,
  "env": {
    "NODE_ENV": "@NODE_ENV",
    "DB_HOST": "@DB_HOST",
    "DB_PORT": "@DB_PORT",
    "DB_USERNAME": "@DB_USERNAME",
    "DB_PASSWORD": "@DB_PASSWORD",
    "JWT_SECRET": "@JWT_SECRET",
    "DB_DATABASE": "@DB_DATABASE"
  },
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ]
}